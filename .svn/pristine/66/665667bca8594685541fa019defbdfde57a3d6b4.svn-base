package com.daweda.model.medyevskiy;


import com.daweda.model.entity.Account;

public class FundManagementModel {

	private int balanse;
	private int closedPosition;
	private int pendingPosition;
	private int openPosition;
	
	private int totalCash;
	private int availableCash;
	private int availableTrades;
	private int processedWithdrawals;
	
	private Account account;

	public FundManagementModel(int balanse, int closedPosition,
			int pendingPosition, int openPosition, int totalCash,
			int availableCash, int availableTrades, int processedWithdrawals,
			Account account) {
		super();
		this.balanse = balanse;
		this.closedPosition = closedPosition;
		this.pendingPosition = pendingPosition;
		this.openPosition = openPosition;
		this.totalCash = totalCash;
		this.availableCash = availableCash;
		this.availableTrades = availableTrades;
		this.processedWithdrawals = processedWithdrawals;
		this.account = account;
	}

	public FundManagementModel() {
		super();
	}
	
	public int getProcessedWithdrawals() {
		return processedWithdrawals;
	}

	public void setProcessedWithdrawals(int processedWithdrawals) {
		this.processedWithdrawals = processedWithdrawals;
	}
	
	public int getBalanse() {
		return balanse;
	}
	public void setBalanse(int balanse) {
		this.balanse = balanse;
	}
	public int getClosedPosition() {
		return closedPosition;
	}
	public void setClosedPosition(int closedPosition) {
		this.closedPosition = closedPosition;
	}
	public int getPendingPosition() {
		return pendingPosition;
	}
	public void setPendingPosition(int pendingPosition) {
		this.pendingPosition = pendingPosition;
	}
	public int getOpenPosition() {
		return openPosition;
	}
	public void setOpenPosition(int openPosition) {
		this.openPosition = openPosition;
	}
	public int getTotalCash() {
		return totalCash;
	}
	public void setTotalCash(int totalCash) {
		this.totalCash = totalCash;
	}
	public int getAvailableCash() {
		return availableCash;
	}
	public void setAvailableCash(int availableCash) {
		this.availableCash = availableCash;
	}
	public int getAvailableTrades() {
		return availableTrades;
	}
	public void setAvailableTrades(int availableTrades) {
		this.availableTrades = availableTrades;
	}

	public Account getAccount() {
		return account;
	}

	public void setAccount(Account account) {
		this.account = account;
	}
	
	
}
